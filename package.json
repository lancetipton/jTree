{
  "name": "jtree",
  "version": "1.5.1",
  "description": "Json and Javascript object editor",
  "author": "Lance Tipton <ltipton@tiptondigital.com>",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/lancetipton/jtree.git"
  },
  "bugs": {
    "url": "https://github.com/lancetipton/jtree/issues"
  },
  "keywords": [
    "json",
    "editor",
    "gui",
    "ui",
    "object editor"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "yarn clean:build; NODE_ENV=production rollup -c ./configs/rollup.config.js",
    "build:modules": "yarn build:esm && yarn build:cjs",
    "clean": "yarn cache clean && rimraf yarn.lock && rimraf node_modules",
    "clean:build": "rimraf ./build/esm && rimraf ./build/cjs",
    "deploy": "gh-pages -d build",
    "dev": "rollup -c ./configs/rollup.config.js -w",
    "test": "NODE_ENV=test jest --no-cache --config=./configs/jest.config.js",
    "web": "http-server ./build -p 8888 --cors"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.8.1",
    "jsutils": "git+https://github.com/lancetipton/jsutils.git"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-transform-object-assign": "7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-property-literals": "7.2.0",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.6.3",
    "@rollup/plugin-alias": "2.2.0",
    "@rollup/plugin-json": "4.0.1",
    "@rollup/plugin-replace": "2.3.0",
    "@fortawesome/fontawesome-free": "5.8.1",
    "app-root-path": "3.0.0",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-import-css-to-jss": "3.1.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "core-js": "3",
    "gh-pages": "2.0.1",
    "jest": "24.7.1",
    "jest-cli": "24.7.1",
    "jsutils": "git+https://github.com/lancetipton/jsutils.git",
    "jt-js-defs": "git+https://github.com/lancetipton/jt-js-defs.git",
    "styleloader": "git+https://github.com/lancetipton/StyleLoader.git",
    "rimraf": "3.0.0",
    "rollup": "1.20.3",
    "rollup-jest": "0.0.2",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-cleanup": "3.1.1",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-terser": "5.1.2"
  }
}
