{
  "name": "jtree",
  "version": "1.1.1",
  "description": "Json and Javascript object editor",
  "author": "Lance Tipton <ltipton@tiptondigital.com>",
  "main": "build/jTree.min.js",
  "module": "src/scripts/index.js",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/lancetipton/jTree.git"
  },
  "bugs": {
    "url": "https://github.com/lancetipton/jTree/issues"
  },
  "keywords": [
    "json",
    "editor",
    "gui",
    "ui",
    "object editor"
  ],
  "scripts": {
    "test": "NODE_ENV=test jest --verbose",
    "dev": "NODE_ENV=development webpack --watch --progress",
    "build": "NODE_ENV=production webpack --mode production",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-property-literals": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "cleave.js": "^1.4.10",
    "copy-webpack-plugin": "^4.6.0",
    "core-js": "3",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "jest-cli": "^24.7.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "raw-loader": "^2.0.0",
    "remove-comments-loader": "^0.1.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "babel-plugin-import-css-to-jss": "^3.1.1",
    "lodash.unset": "^4.5.2"
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.js?(x)"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/loader.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "es6"
    ],
    "globals": {
      "__DEV__": true
    },
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  }
}
