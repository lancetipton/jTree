{
  "presets": [ 
    [
      "@babel/preset-env",
      {
        "corejs": "3.0.0",
        "modules": false,
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-property-literals",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-object-assign",
    "@babel/plugin-syntax-dynamic-import",
    [ "@babel/plugin-proposal-class-properties" ]
  ]
}